[push]
	default = simple

[color]
	ui = true

[diff]
	renames = true
  tool = vimdiff

[user]
	name = Chris Clonch

[include]
  path = ~/.config/git/aliases
  path = ~/.config/git.local
[credential]
	helper = !aws codecommit credential-helper $@
	UseHttpPath = true
[remote "origin"]
	prune = true
[init]
	defaultBranch = main
